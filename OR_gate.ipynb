   "source": [
    "def OR_gate(x1, x2) :\n",
    "# weights\n",
    "  w1 = 0.6\n",
    "  w2 = 0.6\n",
    "  bias = -0.5\n",
    "\n",
    "  # forward\n",
    "  result = x1*w1 + x2*w2 + bias\n",
    "\n",
    "  # activation\n",
    "  if result > 0:\n",
    "    return 1\n",
    "  else:\n",
    "    return 0\n",
    "\n",
    "print(OR_gate(0,0), OR_gate(0,1), OR_gate(1,0), OR_gate(1,1))"
   ]
