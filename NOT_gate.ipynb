    "def NOT_gate(x1) :\n",
    "# weights\n",
    "  w1 = -0.6\n",
    "  bias = 0.5\n",
    "\n",
    "  # forward\n",
    "  result = x1*w1 + bias\n",
    "\n",
    "  # activation\n",
    "  if result > 0:\n",
    "    return 1\n",
    "  else:\n",
    "    return 0\n",
    "\n",
    "NOT_gate(0),NOT_gate(1)"
   ]
  }
